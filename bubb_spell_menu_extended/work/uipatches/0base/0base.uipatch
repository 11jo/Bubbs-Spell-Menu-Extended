+@0             res = res..(dot == "" and ("%03d%s"):format(#m, m)
+@0                                   or "."..n)
+@0             dot, c = c:match"(%.?)(.*)"
+@0          end
+@0          res = res..c:gsub(".", "\0%0")
+@0       end
+@0       return res
+@0    end
+@0    table.sort(o,
+@0       function (a, b)
+@0          local ca, cb = conv(a.string), conv(b.string)
+@0          return ca < cb or ca == cb and a.string < b.string
+@0       end)

function B3printEntries(entriesTable, indentLevel, indentStrings, previousState, levelTableKey)

	local tableEntryKey, tableEntry = next(entriesTable, levelTableKey)
	while(tableEntry ~= nil) do

		local tableEntryString = tableEntry.string
		local tableEntryValue = tableEntry.value
		local indentString = indentStrings[indentLevel]

		if tableEntryValue ~= nil then

			if type(tableEntryValue) ~= 'table' then

				local valueToPrint = string.gsub(tostring(tableEntryValue), '\n', '\\n')
				print(indentString..tableEntryString..valueToPrint)
			else
	
				print(indentString..tableEntryString)
				print(indentString..'{')

				local previous = {}
				previous.entriesTable = entriesTable
				previous.indentLevel = indentLevel
				previous.levelTableKey = tableEntryKey
				previous.previousState = previousState

				indentLevel = indentLevel + 1

				local indentStringsSize = #indentStrings
				if indentLevel > indentStringsSize then
					indentStrings[indentStringsSize + 1] = indentStrings[indentStringsSize]..'\t'
				end

				return B3printEntries(tableEntryValue, indentLevel, indentStrings, previous)
			end
		else
			print(indentString..tableEntryString)
		end

		--Increment
		tableEntryKey, tableEntry = next(entriesTable, tableEntryKey)
		--Increment
	end

	print(indentStrings[indentLevel - 1]..'}')

	--Finish previous levels
	if previousState ~= nil then

		return B3printEntries(previousState.entriesTable, previousState.indentLevel, indentStrings,
							previousState.previousState, previousState.levelTableKey)
	end
end
function B3replaceSpellUUIDs(string)

	local toReturn = string
	local spellUUIDIndexPairs = B3findAllPairs(string, 'SPELL_UUID:')

	for spellUUIDIndexPairKey, spellUUIDIndexPair in pairs(spellUUIDIndexPairs) do

		local spellUUIDIndex = spellUUIDIndexPair[1]
		local spellUUIDStatement = string.sub(string, spellUUIDIndex, spellUUIDIndex + 13)
		local spellUUID = string.sub(spellUUIDStatement, 12, 15)
		local spellName = B3spellInfo[spellUUID].name
		toReturn = string.gsub(toReturn, spellUUIDStatement, spellName)
	end
	return toReturn
end
function B3resetSpellHotbar()

	if not B3isTooltipArrow(actionBarTooltip[0]) then
		return
	end

	local oldTooltip = nil
	local newTooltip = nil

	repeat

		oldTooltip = actionBarTooltip[10]
		buttonArray:OnLButtonPressed(0)
		newTooltip = actionBarTooltip[10]

	until newTooltip == oldTooltip
end
function B3resolveTooltip(index)
	local tooltip = actionBarTooltip[index]
	tooltip = B3replaceSpellUUIDs(tooltip)
	return tooltip
end
function B3selectSpellWithUUID(uuid, leftClick)

	B3resetSpellHotbar()
	local finding = true
	local findStartIndex = 0
	while finding == true do

		for i = findStartIndex, 11, 1 do

			if buttonArray:GetButtonEnabled(i) then

				local tooltipValue = actionBarTooltip[i]

				if B3isTooltipSpell(tooltipValue) then

					local spellUUID = B3getSpellUUID(tooltipValue)

					if spellUUID == uuid then

						if leftClick then
							buttonArray:OnLButtonPressed(i)
						else
							buttonArray:OnRButtonPressed(i)
						end

						finding = false
						break
					end
				end
			else
				finding = false
				break
			end
		end

		if finding then

			local newSpellIndex = B3findNewSpellIndex()

			if newSpellIndex ~= nil then
				findStartIndex = newSpellIndex
			else
				B3resetSpellHotbar()
				finding = false
			end
		end
	end
end
function B3sortFilteredSpellListInfo()
	for i = 1, #B3filteredSpellListInfo, 1 do
		B3alphanumericSortSpellInfo(B3filteredSpellListInfo[i])
	end
end
function B3worldMessageBoxOverride()
	if worldMessageBoxText ~= B3previousWorldMessageBoxText then
		B3worldMessageBoxTextOverride = B3replaceSpellUUIDs(worldMessageBoxText)
		B3previousWorldMessageBoxText = worldMessageBoxText
	end
	return B3worldMessageBoxTextOverride
end

	adjustItemGroup({"messagesHandleY"},0,newY,0,0)
	adjustItemGroup({"messagesRect","worldMessageBox"},0,newY,0,-newY)

	chatboxScrollToBottom = 1
	worldMessageBoxTop = y + newY
end

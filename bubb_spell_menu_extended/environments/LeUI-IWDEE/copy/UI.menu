function B3alphanumericSortEntries(o)
   local function conv(s)
      local res, dot = "", ""
      for n, m, c in tostring(s):gmatch"(0*(%d*))(.?)" do
         if n == "" then
            dot, c = "", dot..c
         else
            res = res..(dot == "" and ("%03d%s"):format(#m, m)
                                  or "."..n)
            dot, c = c:match"(%.?)(.*)"
         end
         res = res..c:gsub(".", "\0%0")
      end
      return res
   end
   table.sort(o,
      function (a, b)
         local ca, cb = conv(a.string), conv(b.string)
         return ca < cb or ca == cb and a.string < b.string
      end)
   return o
end
function B3alphanumericSortSpellInfo(o)
   local function conv(s)
      local res, dot = "", ""
      for n, m, c in tostring(s):gmatch"(0*(%d*))(.?)" do
         if n == "" then
            dot, c = "", dot..c
         else
            res = res..(dot == "" and ("%03d%s"):format(#m, m)
                                  or "."..n)
            dot, c = c:match"(%.?)(.*)"
         end
         res = res..c:gsub(".", "\0%0")
      end
      return res
   end
   table.sort(o,
      function (a, b)
         local ca, cb = conv(a.spellName), conv(b.spellName)
         return ca < cb or ca == cb and a.spellName < b.spellName
      end)
   return o
end
function B3centerItemsX(names)
	local greatestX = 0
	for i = 1, #names, 1 do
		local name = names[i]
		local itemX, itemY, itemWidth, itemHeight = Infinity_GetArea(name.name)
		local itemRightEdge = name.x + itemWidth
		if itemRightEdge > greatestX then greatestX = itemRightEdge end
	end
	local screenWidth, screenHeight = Infinity_GetScreenSize()
	local centerXStart = screenWidth / 2 - greatestX / 2
	for i = 1, #names, 1 do
		local name = names[i]
		Infinity_SetArea(name.name, centerXStart + name.x, nil, nil, nil)
	end
end
function B3centerItemsXY(names)
	local greatestX = 0
	local greatestY = 0
	for i = 1, #names, 1 do
		local name = names[i]
		local itemRightEdge = name.x + name.width
		if itemRightEdge > greatestX then greatestX = itemRightEdge end
		local itemBottomEdge = name.y + name.height
		if itemBottomEdge > greatestY then greatestY = itemBottomEdge end
	end
	local screenWidth, screenHeight = Infinity_GetScreenSize()
	local centerXStart = screenWidth / 2 - greatestX / 2
	local centerYStart = screenHeight / 2 - greatestY / 2
	for i = 1, #names, 1 do
		local name = names[i]
		Infinity_SetArea(name.name, centerXStart + name.x, centerYStart + name.y, name.width, name.height)
	end
end
function B3checkForOpcode214Watchdog()

	if B3checkForOpcode214 then
		if not B3isFirstTooltipTalk() then
			B3launchSpellMenu()
		end
		B3checkForOpcode214 = false
	end
end
function B3customSpellMenuWatchdog()

	if B3checkForSpellMenuUpdate then
		if B3isFirstTooltipTalk() then
			Infinity_PopMenu("B3_SPELL_MENU_SLOTS")
		else

			B3oldSpellSearchEdit = ''
			B3spellSearchEdit = ''
			Infinity_FocusTextEdit('B3spellMenuSearch');

			B3fillSpellListInfo()
			B3initializeSlots()
		end
		B3checkForSpellMenuUpdate = false
	elseif B3isFirstTooltipTalk() then
		Infinity_PopMenu("B3_SPELL_MENU_SLOTS")
	end

	if B3spellSearchEdit ~= B3oldSpellSearchEdit then
		if B3spellSearchEdit ~= '' then
			B3filterSpellListInfoSearch()
		else
			B3filterSpellListInfoAll()
		end
		B3oldSpellSearchEdit = B3spellSearchEdit
	end
end
function B3decrementRowOffset(slot_row)
	local rowOffset = B3slotLevelInfo[slot_row].spellOffset - B3slotsAvailable + 3
	if rowOffset < 0 then rowOffset = 0 end
	B3slotLevelInfo[slot_row].spellOffset = rowOffset
end
function B3doActionAltForSlot(slot_row, slot_column)
	if not B3getSlotIsArrow(slot_row, slot_column) then
		local spell = B3getSpellForSlot(slot_row, slot_column)
		if spell ~= nil then
			B3selectSpellWithUUID(spell.spellUUID, false)
		end
	end
end
function B3doActionForSlot(slot_row, slot_column)
	if B3getSlotIsArrow(slot_row, slot_column) then
		if slot_column == 2 then
			B3decrementRowOffset(slot_row)
		else
			B3incrementRowOffset(slot_row)
		end
	elseif slot_column ~= 1 then
		local spell = B3getSpellForSlot(slot_row, slot_column)
		if spell ~= nil then
			B3selectSpellWithUUID(spell.spellUUID, true)
			B3checkForSpellMenuUpdate = true
		end
	end
end
function B3dump(key, valueToDump)

	local valueToDumpType = type(valueToDump)

	if valueToDumpType == 'string' or valueToDumpType == 'number' or valueToDumpType == 'boolean' then

		print(valueToDumpType..' '..key..' = '..valueToDump)

	elseif valueToDumpType == 'table' then

		print(valueToDumpType..' '..key..':')
		print('{')
		local entries = B3fillDumpLevel(key, valueToDump, {})
		B3printEntries(entries, 1, {[0] = '', [1] = '\t'})

	elseif valueToDumpType == 'userdata' then

		local metatable = getmetatable(valueToDump)

		if metatable ~= nil then

			print(valueToDumpType..' '..key..':')
			print('{')
			local entries = B3fillDumpLevel(key, metatable, {})
			B3printEntries(entries, 1, {[0] = '', [1] = '\t'})
		else

			print(valueToDumpType..' '..key..' = nil')
		end
	else

		print(valueToDumpType..' '..key)
	end
end
function B3extractSpellTooltipContents()
	local toReturn = {}
	for i = 1, 10, 1 do
		table.insert(toReturn, B3getTooltipContents(actionBarTooltip[i]))
	end
	return toReturn
end
function B3fillDumpLevel(tableName, levelTable, levelToFill, levelTableKey)

	local tableKey, tableValue = next(levelTable, levelTableKey)
	while tableValue ~= nil do

		local tableValueType = type(tableValue)

		if tableValueType == 'string' or tableValueType == 'number' or tableValueType == 'boolean' then

			local entry = {}
			entry.string = tableValueType..' '..tableKey..' = '
			entry.value = tableValue
			table.insert(levelToFill, entry)

		elseif tableValueType == 'table' then

			if tableKey ~= '_G' then

				local entry = {}
				entry.string = tableValueType..' '..tableKey..':\n'
				entry.value = {} --entry.value is a levelToFill

				entry.value.previous = {}
				entry.value.previous.tableName = tableName
				entry.value.previous.levelTable = levelTable
				entry.value.previous.levelToFill = levelToFill
				entry.value.previous.levelTableKey = tableKey

				table.insert(levelToFill, entry)

				return B3fillDumpLevel(tableKey, tableValue, entry.value)
			end
		elseif tableValueType == 'userdata' then

			local metatable = getmetatable(tableValue)
			local entry = {}

			if metatable ~= nil then

				entry.string = tableValueType..' '..tableKey..':\n'
				entry.value = {} --entry.value is a levelToFill

				entry.value.previous = {}
				entry.value.previous.tableName = tableName
				entry.value.previous.levelTable = levelTable
				entry.value.previous.levelToFill = levelToFill
				entry.value.previous.levelTableKey = tableKey

				table.insert(levelToFill, entry)

				return B3fillDumpLevel(tableKey, metatable, entry.value)
			else

				entry.string = tableValueType..' '..tableKey..' = '
				entry.value = 'nil'
				table.insert(levelToFill, entry)
			end
		else

			local entry = {}
			entry.string = tableValueType..' '..tableKey
			entry.value = nil
			table.insert(levelToFill, entry)
		end

		--Iteration
		tableKey, tableValue = next(levelTable, tableKey)
		--Iteration
	end

	--Sort the now finished level
	B3alphanumericSortEntries(levelToFill)
	--Sort the now finished level

	local previous = levelToFill.previous

	if previous ~= nil then

		--Clear out "previous" metadata, as it is no longer needed.
		local previousTableName = previous.tableName
		local previousLevelTable = previous.levelTable
		local previousLevelToFill = previous.levelToFill
		local previousLevelTableKey = previous.levelTableKey
		levelToFill.previous = nil
		--Clear out "previous" metadata, as it is no longer needed.

		return B3fillDumpLevel(previousTableName, previousLevelTable,
							 previousLevelToFill, previousLevelTableKey)
	else
		return levelToFill
	end
end
function B3fillSpellListInfo()

	characters = {}
	Infinity_UpdateLuaStats()

	B3spellListInfo = {[1] = {}, [2] = {}, [3] = {}, [4] = {}, [5] = {},
					 [6] = {}, [7] = {}, [8] = {}, [9] = {}}

	local finding = true
	local findStartIndex = 0
	while finding == true do

		for i = findStartIndex, 11, 1 do

			if buttonArray:GetButtonEnabled(i) then

				local tooltipValue = actionBarTooltip[i]

				if B3isTooltipSpell(tooltipValue) then

					local spellUUID = B3getSpellUUID(tooltipValue)
					local spellResref = B3spellInfo[spellUUID].resref
					local spellName = B3spellInfo[spellUUID].name
					local spellIcon = B3spellInfo[spellUUID].spellIcon
					local spellLevel = B3spellInfo[spellUUID].spellLevel
					local spellType = B3spellInfo[spellUUID].spellType

					local toInsert = {}
					toInsert.spellUUID = spellUUID
					toInsert.spellName = spellName
					toInsert.spellIcon = spellIcon
					toInsert.spellLevel = spellLevel
					toInsert.spellType = spellType
					toInsert.spellCastableCount = B3getSpellCastableCountFromResref(spellResref)

					if spellLevel ~= 0 then
						table.insert(B3spellListInfo[spellLevel], toInsert)
					else
						Infinity_DisplayString('[Bubb\'s Spell Menu] Spell with level 0 encountered! If you see this in-game, please notify @Bubb of this error on the Beamdog forums.')
					end
				end
			else
				finding = false
				break
			end
		end

		if finding then

			local newSpellIndex = B3findNewSpellIndex()

			if newSpellIndex ~= nil then
				findStartIndex = newSpellIndex
			else
				finding = false
			end
		end
	end
	B3filteredSpellListInfo = B3spellListInfo
	B3sortFilteredSpellListInfo()
	B3resetSpellHotbar()
end

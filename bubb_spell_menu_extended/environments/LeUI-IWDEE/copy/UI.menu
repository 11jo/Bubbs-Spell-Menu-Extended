function B3getOptimalSlotSize()
	
	local screenWidth, screenHeight = Infinity_GetScreenSize()
	local sidebarX, sidebarY, sidebarWidth, sidebarHeight = Infinity_GetArea('leftSidebarBackground')
	local horizontalAvailableSpace = screenWidth - (sidebarWidth * 2)

	for tempSlotSize = 52, B3slotSizeMinimum, -1 do

		local totalLinesNeeded = 0
		local tempSlotsAvailable = math.floor(horizontalAvailableSpace / (tempSlotSize * 0.935))

		for i = 1, 9, 1 do
			local spellListInfo = B3filteredSpellListInfo[i] -- REFACTORING: Should this be local?
			local spellCountForLevel = #spellListInfo
			while spellCountForLevel > 0 do
				spellCountForLevel = spellCountForLevel - tempSlotsAvailable + 1
				totalLinesNeeded = totalLinesNeeded + 1
			end
		end

		local verticalAreaAvailable = screenHeight - 83
		local verticalAreaUsed = totalLinesNeeded * tempSlotSize

		if verticalAreaAvailable > verticalAreaUsed then
			return tempSlotSize
		end
	end
	return 52
end
function B3getSlotEnabled(slot_row, slot_column)
	local slotInfo = B3slotLevelInfo[slot_row]
	return slotInfo ~= nil 
		   and not (slot_column == 1 and slotInfo.isFlowover)
		   and B3slotsAvailable >= slot_column 
		   and slotInfo.slotCount >= slot_column
end
function B3getSlotEndsAreArrows(slot_row)
	local slotInfo = B3slotLevelInfo[slot_row]
	return slotInfo ~= nil and slotInfo.slotCount > B3slotsAvailable
end
function B3getSlotInfoOffset(slot_row, slot_column)

	local slotInfo = B3slotLevelInfo[slot_row]
	if slotInfo == nil then return nil end

	if B3getSlotEndsAreArrows(slot_row) then
		return slotInfo.spellOffsetBase + slotInfo.spellOffset + slot_column - 1
	else
		return slotInfo.spellOffsetBase + slotInfo.spellOffset + slot_column
	end
end
function B3getSlotIsArrow(slot_row, slot_column)
	return B3getSlotEndsAreArrows(slot_row) and (slot_column == 2 or slot_column == B3slotsAvailable)
end
function B3getSlotsHorizontalAreaUsed()
	local longestCount = B3getLongestSpellCount()
	if longestCount > B3slotsAvailable then longestCount = B3slotsAvailable end
	local horizontalAreaUsed = 0
	for row = 1, longestCount, 1 do
		horizontalAreaUsed = math.floor(horizontalAreaUsed + B3slotSize * 0.95 - 65 / B3slotSize)
	end
	return horizontalAreaUsed
end
function B3getSlotsVerticalAreaUsed()
	local tempCurrentYOffset = 0
	for row = 1, #B3slotLevelInfo, 1 do
		local nextSlotLevelInfo = B3slotLevelInfo[row + 1]
		if nextSlotLevelInfo ~= nil and nextSlotLevelInfo.isFlowover then
			tempCurrentYOffset = math.floor(tempCurrentYOffset + B3slotSize * 0.95 - 65 / B3slotSize)
		else
			tempCurrentYOffset = tempCurrentYOffset + math.floor(B3slotSize * 1.05)
		end
	end
	return tempCurrentYOffset
end
function B3getSpellCastableCountFromResref(resref)
	for key, character in pairs(characters) do
		for spellLevelIndex = 1, 9, 1 do
			local spellLevel = character.mageSpells[spellLevelIndex]
			for spellIndex = 1, #spellLevel, 1 do
				local spell = spellLevel[spellIndex]
				if spell.resref == resref then
					return spell.castableCount
				end
			end
		end
		for spellLevelIndex = 1, 7, 1 do
			local spellLevel = character.priestSpells[spellLevelIndex]
			for spellIndex = 1, #spellLevel, 1 do
				local spell = spellLevel[spellIndex]
				if spell.resref == resref then
					return spell.castableCount
				end
			end
		end
		return nil
	end
end

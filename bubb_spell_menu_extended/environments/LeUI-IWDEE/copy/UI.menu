function B3findAllPairs(string, toFind)

	local foundIndexPairs = {}
	local startIndex, endIndex = string.find(string, toFind)

	while(startIndex ~= nil) do

		table.insert(foundIndexPairs, table.pack(startIndex, endIndex))
		startIndex, endIndex = string.find(string, toFind, endIndex + 1)
	end

	return foundIndexPairs
end
function B3findFirst(string, toFind)

	local indexLimit = #string - #toFind + 1

	for index = 1, indexLimit, 1 do

		local substring = string.sub(string, index, index + #toFind - 1)

		if(substring == toFind) then

			return index
		end
	end
	return -1
end
function B3findNewSpellIndex()

	if not B3isTooltipArrow(actionBarTooltip[0]) then
		return nil
	end

	local oldTooltips = B3extractSpellTooltipContents()
	local oldTooltip = oldTooltips[10]

	buttonArray:OnLButtonPressed(11)

	local newTooltips = B3extractSpellTooltipContents()
	local newTooltip = newTooltips[10]

	if newTooltip ~= oldTooltip then
		for i = 1, 10, 1 do
			local newValue = newTooltips[i]
			local isNew = true
			for j = 1, 10, 1 do
				local oldValue = oldTooltips[j]
				if newValue == oldValue then
					isNew = false
					break
				end
			end
			if isNew then
				return i
			end
		end
	else
		return nil
	end
end

function B3initializeSlots()

	if B3automaticallyOptimizeSlotSize == 1 then
		B3slotSize = B3getOptimalSlotSize()
		B3slotSizeSlider = B3slotSize - B3slotSizeMinimum
	end

	local screenWidth, screenHeight = Infinity_GetScreenSize()
	local sidebarX, sidebarY, sidebarWidth, sidebarHeight = Infinity_GetArea('leftSidebarBackground')

	local horizontalAvailableSpace = screenWidth - (sidebarWidth * 2)
	B3slotsAvailable = math.floor(horizontalAvailableSpace / (B3slotSize * 0.935))
	if B3slotsAvailable > 50 then B3slotsAvailable = 50 end

	local linesPossible = math.floor((screenHeight - 83) / (B3slotSize))
	if linesPossible > 18 then linesPossible = 18 end

	local filledSpellLevels = 0
	for i = 1, #B3filteredSpellListInfo, 1 do 
		if #B3filteredSpellListInfo[i] > 0 then
			filledSpellLevels = filledSpellLevels + 1
		end
	end
	local flowoverLinesAvailable = linesPossible - filledSpellLevels

	B3slotLevelInfo = {}
	local flowoverLinesCounter = flowoverLinesAvailable
	for i = 1, 9, 1 do
		local spellCount = #B3filteredSpellListInfo[i]
		if spellCount > 0 then
			local spellOffset = 0
			local levelRowCount = 0
			while true do
				levelRowCount = levelRowCount + 1
				local spellLevelInfo = {}
				if levelRowCount > 1 then
					if flowoverLinesCounter < 1 then
						local previousSlotCount = B3slotLevelInfo[#B3slotLevelInfo].slotCount
						B3slotLevelInfo[#B3slotLevelInfo].slotCount = previousSlotCount + spellCount
						break
					end
					spellLevelInfo.isFlowover = true
					flowoverLinesCounter = flowoverLinesCounter - 1
				else
					spellLevelInfo.isFlowover = false
				end
				spellLevelInfo.spellLevel = i
				spellLevelInfo.spellOffsetBase = spellOffset
				spellLevelInfo.spellOffset = 0
				if spellCount >= B3slotsAvailable then

					spellLevelInfo.slotCount = B3slotsAvailable
					table.insert(B3slotLevelInfo, spellLevelInfo)
					spellOffset = spellOffset + B3slotsAvailable - 1
					spellCount = spellCount - B3slotsAvailable + 1

				else
					spellLevelInfo.slotCount = spellCount + 1
					table.insert(B3slotLevelInfo, spellLevelInfo)
					break
				end
			end
		end
	end

	local slotsXOffset = nil
	if B3alignCenter then

		local horizontalAreaUsed = B3getSlotsHorizontalAreaUsed()
		local horizontalMarginSpace = horizontalAvailableSpace - horizontalAreaUsed
		slotsXOffset = sidebarWidth + (horizontalMarginSpace / 2)
	else
		slotsXOffset = sidebarWidth
	end

	local verticalAreaUsed = B3getSlotsVerticalAreaUsed()
	local verticalMarginSpace = screenHeight - 83 - verticalAreaUsed
	local currentYOffset = 83 + (verticalMarginSpace / 2)

	Infinity_SetArea('B3spellMenuOptions', screenWidth - sidebarWidth - 72, nil, nil, nil)

	if B3alignCenter then

		local x, y, bubbSpellMenuBackgroundWidth, h = Infinity_GetArea('B3spellMenuSearchBackground')
		local bubbSpellMenuXPos = Infinity_GetScreenSize() / 2 - bubbSpellMenuBackgroundWidth / 2

		Infinity_SetArea('B3spellMenuSearchBackground', bubbSpellMenuXPos, nil, nil, nil)
		Infinity_SetArea('B3spellMenuSearch', 		  bubbSpellMenuXPos, nil, nil, nil)

		B3centerItemsX(
		{
			{ ['name'] = 'B3moveSlotsLeft',     ['x'] = 0   },
			{ ['name'] = 'B3filterSlotsMage',   ['x'] = 52  },
			{ ['name'] = 'B3filterSlotsAll',    ['x'] = 104 },
			{ ['name'] = 'B3filterSlotsCleric', ['x'] = 156 },
			{ ['name'] = 'B3slotSizeSlider',    ['x'] = 208 },
			{ ['name'] = 'B3optimizeSlotSize',  ['x'] = 460 },
		})

	else

		Infinity_SetArea('B3spellMenuSearchBackground', sidebarWidth, nil, nil, nil)
		Infinity_SetArea('B3spellMenuSearch', 		  sidebarWidth, nil, nil, nil)

		Infinity_SetArea('B3filterSlotsMage',                     sidebarWidth,       nil, nil, nil)
		Infinity_SetArea('B3filterSlotsAll',                      sidebarWidth + 52,  nil, nil, nil)
		Infinity_SetArea('B3filterSlotsCleric',                   sidebarWidth + 104, nil, nil, nil)
		Infinity_SetArea('B3moveSlotsRight',                      sidebarWidth + 156, nil, nil, nil)
		Infinity_SetArea('B3slotSizeSlider',                      sidebarWidth + 208, nil, nil, nil)
		Infinity_SetArea('B3optimizeSlotSize',                    sidebarWidth + 460, nil, nil, nil)

	end

	local slotSizeIconSize = math.floor(B3slotSize * 0.8)
	local slotIconCenterOffset = (B3slotSize - slotSizeIconSize) / 2
	local slotRowCount = #B3slotLevelInfo

	local longestCount = B3getLongestSpellCount()
	if longestCount > B3slotsAvailable then longestCount = B3slotsAvailable end

	for row = 1, slotRowCount, 1 do

		local currentXOffset = slotsXOffset

		for column = 1, longestCount + 1, 1 do
			Infinity_SetArea('B3slot_'..row..'_'..column..'A', 
				currentXOffset, 
				currentYOffset, 
				B3slotSize, 
				B3slotSize)
			Infinity_SetArea('B3slot_'..row..'_'..column..'B', 
				currentXOffset, 
				currentYOffset, 
				B3slotSize, 
				B3slotSize)
			Infinity_SetArea('B3slot_'..row..'_'..column..'C', 
				currentXOffset + slotIconCenterOffset, 
				currentYOffset + slotIconCenterOffset, 
				slotSizeIconSize, 
				slotSizeIconSize)
			currentXOffset = math.floor(currentXOffset + B3slotSize * 0.95 - 65 / B3slotSize)
		end

		local nextSlotLevelInfo = B3slotLevelInfo[row + 1]
		if nextSlotLevelInfo ~= nil and nextSlotLevelInfo.isFlowover then
			currentYOffset = math.floor(currentYOffset + B3slotSize * 0.95 - 65 / B3slotSize)
		else
			currentYOffset = currentYOffset + math.floor(B3slotSize * 1.05)
		end
	end
end

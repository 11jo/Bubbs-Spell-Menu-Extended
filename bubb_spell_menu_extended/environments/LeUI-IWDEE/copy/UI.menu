	local amt = tonumber(itemRequestAmt) or 0
	if minus then return amt > 1 end
	return amt < requester.requesterMax
end
function requesterPlusMinusButtonClick(minus)
	local amt = tonumber(itemRequestAmt) or 0
	local delta = 1
	if minus then delta = -1 end
	itemRequestAmt = amt + delta
